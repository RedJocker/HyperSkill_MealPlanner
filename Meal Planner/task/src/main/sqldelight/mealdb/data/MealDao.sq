selectAll:
SELECT *
FROM mealDao;

insert:
INSERT INTO mealDao(category, meal)
VALUES (?, ?);

findIdByName:
SELECT meal_id FROM mealDao
WHERE meal = (?);

findMealDao:
SELECT * FROM mealDao
WHERE meal = (?) AND category = (?);





